<h2>Edit House</h2>

<%=form_for @house do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :photo_url %>
<%= f.text_field :photo_url %>

<%= f.label :slogan %>
<%= f.text_field :slogan %>

<%= f.submit %>

<% end %>

<%= form_for @house, method: :delete do |f| %>
<%= f.submit "Delete", class: "delete" %>
<% end %>
